#!/bin/sh
# Vectorize data file
echo "Begin vectorizing: " `date`
gunzip -c source/ipumsi_00054.dat.gz > data.dat
sha1sum data.dat > source/ipumsi_0005.sha1
mkdir data
cut -c1-3 data.dat | gzip -c - > data/cntry.gz
cut -c4-7 data.dat | gzip -c - > data/year.gz
cut -c8-11 data.dat | gzip -c - > data/sample.gz
cut -c12-21 data.dat | gzip -c - > data/serial.gz
cut -c22-28 data.dat | gzip -c - > data/dwnum.gz
cut -c29-30 data.dat | gzip -c - > data/pern.gz
cut -c31-31 data.dat | gzip -c - > data/fbig.gz
cut -c32-32 data.dat | gzip -c - > data/dept.gz
cut -c33-34 data.dat | gzip -c - > data/dwtype.gz
cut -c35-35 data.dat | gzip -c - > data/occup.gz
cut -c36-36 data.dat | gzip -c - > data/wall.gz
cut -c37-37 data.dat | gzip -c - > data/wallint.gz
cut -c38-38 data.dat | gzip -c - > data/roof.gz
cut -c39-39 data.dat | gzip -c - > data/floor.gz
cut -c40-40 data.dat | gzip -c - > data/watsrc.gz
cut -c41-41 data.dat | gzip -c - > data/watpipe.gz
cut -c42-42 data.dat | gzip -c - > data/toilet1.gz
cut -c43-43 data.dat | gzip -c - > data/toilet2.gz
cut -c44-44 data.dat | gzip -c - > data/sewage.gz
cut -c45-45 data.dat | gzip -c - > data/electric.gz
cut -c46-46 data.dat | gzip -c - > data/fuelcook.gz
cut -c47-47 data.dat | gzip -c - > data/kitchen.gz
cut -c48-48 data.dat | gzip -c - > data/rooms.gz
cut -c49-49 data.dat | gzip -c - > data/bedrooms.gz
cut -c50-50 data.dat | gzip -c - > data/radio.gz
cut -c51-51 data.dat | gzip -c - > data/tv.gz
cut -c52-52 data.dat | gzip -c - > data/bike.gz
cut -c53-53 data.dat | gzip -c - > data/motbike.gz
cut -c54-54 data.dat | gzip -c - > data/auto.gz
cut -c55-55 data.dat | gzip -c - > data/refrig.gz
cut -c56-56 data.dat | gzip -c - > data/phone.gz
cut -c57-57 data.dat | gzip -c - > data/pumpwat.gz
cut -c58-58 data.dat | gzip -c - > data/ownershp.gz
cut -c59-59 data.dat | gzip -c - > data/blind.gz
cut -c60-60 data.dat | gzip -c - > data/deaf.gz
cut -c61-61 data.dat | gzip -c - > data/paralys.gz
cut -c62-62 data.dat | gzip -c - > data/death.gz
cut -c63-63 data.dat | gzip -c - > data/deathf.gz
cut -c64-64 data.dat | gzip -c - > data/deathfres.gz
cut -c65-66 data.dat | gzip -c - > data/deathfage.gz
cut -c67-67 data.dat | gzip -c - > data/urban.gz
cut -c68-70 data.dat | gzip -c - > data/pernum.gz
cut -c71-78 data.dat | gzip -c - > data/wtper.gz
cut -c79-80 data.dat | gzip -c - > data/pernum.gz
cut -c81-81 data.dat | gzip -c - > data/sex.gz
cut -c82-83 data.dat | gzip -c - > data/age.gz
cut -c84-84 data.dat | gzip -c - > data/birthreg.gz
cut -c85-86 data.dat | gzip -c - > data/relate.gz
cut -c87-87 data.dat | gzip -c - > data/lang1.gz
cut -c88-88 data.dat | gzip -c - > data/lang2.gz
cut -c89-89 data.dat | gzip -c - > data/lang3.gz
cut -c90-90 data.dat | gzip -c - > data/lang4.gz
cut -c91-91 data.dat | gzip -c - > data/lang5.gz
cut -c92-92 data.dat | gzip -c - > data/lang6.gz
cut -c93-93 data.dat | gzip -c - > data/lang7.gz
cut -c94-94 data.dat | gzip -c - > data/livehere.gz
cut -c95-97 data.dat | gzip -c - > data/countryh.gz
cut -c98-99 data.dat | gzip -c - > data/depth.gz
cut -c100-103 data.dat | gzip -c - > data/provh.gz
cut -c104-104 data.dat | gzip -c - > data/bornhere.gz
cut -c105-107 data.dat | gzip -c - > data/ctrybrth.gz
cut -c108-111 data.dat | gzip -c - > data/yrarriv.gz
cut -c112-113 data.dat | gzip -c - > data/deptbrth.gz
cut -c114-117 data.dat | gzip -c - > data/provbrth.gz
cut -c118-118 data.dat | gzip -c - > data/mothtong.gz
cut -c119-119 data.dat | gzip -c - > data/lit.gz
cut -c120-120 data.dat | gzip -c - > data/attend.gz
cut -c121-121 data.dat | gzip -c - > data/levatt.gz
cut -c122-123 data.dat | gzip -c - > data/levcomp.gz
cut -c124-125 data.dat | gzip -c - > data/yrcomp.gz
cut -c126-128 data.dat | gzip -c - > data/edattan.gz
cut -c129-130 data.dat | gzip -c - > data/prereq.gz
cut -c131-132 data.dat | gzip -c - > data/prereqyr.gz
cut -c133-133 data.dat | gzip -c - > data/here5yr.gz
cut -c134-136 data.dat | gzip -c - > data/cntry5yr.gz
cut -c137-138 data.dat | gzip -c - > data/dept5yr.gz
cut -c139-142 data.dat | gzip -c - > data/prov5yr.gz
cut -c143-143 data.dat | gzip -c - > data/wkrlstwk.gz
cut -c144-144 data.dat | gzip -c - > data/nonwork1.gz
cut -c145-145 data.dat | gzip -c - > data/nonwork2.gz
cut -c146-148 data.dat | gzip -c - > data/occ.gz
cut -c149-149 data.dat | gzip -c - > data/classwk.gz
cut -c150-152 data.dat | gzip -c - > data/ind3.gz
cut -c153-153 data.dat | gzip -c - > data/marst.gz
cut -c154-154 data.dat | gzip -c - > data/indig.gz
cut -c155-156 data.dat | gzip -c - > data/chborn.gz
cut -c157-158 data.dat | gzip -c - > data/chsurv.gz
cut -c159-160 data.dat | gzip -c - > data/chdead.gz
cut -c161-162 data.dat | gzip -c - > data/bthmonth.gz
cut -c163-166 data.dat | gzip -c - > data/bthyear.gz
cut -c167-167 data.dat | gzip -c - > data/bpl.gz
cut -c168-168 data.dat | gzip -c - > data/birthdr.gz
rm data.dat
echo "  End vectorizing: " `date`
