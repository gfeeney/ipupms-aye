#!/bin/sh
# Vectorize data file
echo "Begin vectorizing: " `date`
gunzip -c source/ipumsi_00068.dat.gz > data.dat
sha1sum data.dat > source/ipumsi_00068.dat.gz.sha1
mkdir data
cut -c1-3 data.dat | gzip -c - > data/cntry.gz
cut -c4-7 data.dat | gzip -c - > data/year.gz
cut -c8-11 data.dat | gzip -c - > data/sample.gz
cut -c12-21 data.dat | gzip -c - > data/serial.gz
cut -c22-28 data.dat | gzip -c - > data/dwnum.gz
cut -c29-30 data.dat | gzip -c - > data/pern.gz
cut -c31-31 data.dat | gzip -c - > data/fbig.gz
cut -c32-34 data.dat | gzip -c - > data/fbig_nh.gz
cut -c35-36 data.dat | gzip -c - > data/gov.gz
cut -c37-37 data.dat | gzip -c - > data/urban.gz
cut -c38-39 data.dat | gzip -c - > data/pubhous.gz
cut -c40-41 data.dat | gzip -c - > data/males.gz
cut -c42-43 data.dat | gzip -c - > data/females.gz
cut -c44-45 data.dat | gzip -c - > data/members.gz
cut -c46-47 data.dat | gzip -c - > data/dwtype.gz
cut -c48-48 data.dat | gzip -c - > data/ownershp.gz
cut -c49-49 data.dat | gzip -c - > data/watsrc.gz
cut -c50-50 data.dat | gzip -c - > data/watacc.gz
cut -c51-51 data.dat | gzip -c - > data/lighting.gz
cut -c52-52 data.dat | gzip -c - > data/sewage.gz
cut -c53-53 data.dat | gzip -c - > data/sewage.gz
cut -c54-54 data.dat | gzip -c - > data/sewage.gz
cut -c55-56 data.dat | gzip -c - > data/rooms.gz
cut -c57-60 data.dat | gzip -c - > data/rent.gz
cut -c61-61 data.dat | gzip -c - > data/kitchen.gz
cut -c62-62 data.dat | gzip -c - > data/bathroom.gz
cut -c63-63 data.dat | gzip -c - > data/bathtoil.gz
cut -c64-64 data.dat | gzip -c - > data/septoil.gz
cut -c65-65 data.dat | gzip -c - > data/propane.gz
cut -c66-66 data.dat | gzip -c - > data/gas.gz
cut -c67-67 data.dat | gzip -c - > data/kerosene.gz
cut -c68-68 data.dat | gzip -c - > data/electric.gz
cut -c69-69 data.dat | gzip -c - > data/othfuel.gz
cut -c70-71 data.dat | gzip -c - > data/auto.gz
cut -c72-73 data.dat | gzip -c - > data/motocyc.gz
cut -c74-75 data.dat | gzip -c - > data/bike.gz
cut -c76-77 data.dat | gzip -c - > data/othtrans.gz
cut -c78-78 data.dat | gzip -c - > data/notrans.gz
cut -c79-80 data.dat | gzip -c - > data/radio.gz
cut -c81-82 data.dat | gzip -c - > data/tv.gz
cut -c83-84 data.dat | gzip -c - > data/video.gz
cut -c85-86 data.dat | gzip -c - > data/satell.gz
cut -c87-88 data.dat | gzip -c - > data/fridge.gz
cut -c89-90 data.dat | gzip -c - > data/freezer.gz
cut -c91-92 data.dat | gzip -c - > data/washernon.gz
cut -c93-94 data.dat | gzip -c - > data/washer.gz
cut -c95-96 data.dat | gzip -c - > data/dishwash.gz
cut -c97-98 data.dat | gzip -c - > data/stove.gz
cut -c99-100 data.dat | gzip -c - > data/watheat.gz
cut -c101-102 data.dat | gzip -c - > data/vacuum.gz
cut -c103-104 data.dat | gzip -c - > data/aircon.gz
cut -c105-106 data.dat | gzip -c - > data/pc.gz
cut -c107-108 data.dat | gzip -c - > data/fan.gz
cut -c109-110 data.dat | gzip -c - > data/internet.gz
cut -c111-112 data.dat | gzip -c - > data/phone.gz
cut -c113-114 data.dat | gzip -c - > data/cell.gz
cut -c115-115 data.dat | gzip -c - > data/noappl.gz
cut -c116-118 data.dat | gzip -c - > data/pernum.gz
cut -c119-126 data.dat | gzip -c - > data/wtper.gz
cut -c127-128 data.dat | gzip -c - > data/pernum.gz
cut -c129-130 data.dat | gzip -c - > data/relate.gz
cut -c131-132 data.dat | gzip -c - > data/age.gz
cut -c133-133 data.dat | gzip -c - > data/sex.gz
cut -c134-134 data.dat | gzip -c - > data/relig.gz
cut -c135-135 data.dat | gzip -c - > data/nationg.gz
cut -c136-138 data.dat | gzip -c - > data/nation.gz
cut -c139-139 data.dat | gzip -c - > data/marst.gz
cut -c140-140 data.dat | gzip -c - > data/wives.gz
cut -c141-142 data.dat | gzip -c - > data/edlevel.gz
cut -c143-143 data.dat | gzip -c - > data/school.gz
cut -c144-144 data.dat | gzip -c - > data/dropstag.gz
cut -c145-145 data.dat | gzip -c - > data/dropgrad.gz
cut -c146-147 data.dat | gzip -c - > data/empstat.gz
cut -c148-148 data.dat | gzip -c - > data/estab.gz
cut -c149-149 data.dat | gzip -c - > data/empsect.gz
cut -c150-152 data.dat | gzip -c - > data/occ.gz
cut -c153-155 data.dat | gzip -c - > data/ind.gz
cut -c156-156 data.dat | gzip -c - > data/disab.gz
cut -c157-158 data.dat | gzip -c - > data/bplgov.gz
cut -c159-162 data.dat | gzip -c - > data/bpldist.gz
cut -c163-163 data.dat | gzip -c - > data/bplurb.gz
cut -c164-166 data.dat | gzip -c - > data/durres.gz
cut -c167-168 data.dat | gzip -c - > data/prevgov.gz
cut -c169-172 data.dat | gzip -c - > data/prevdist.gz
cut -c173-173 data.dat | gzip -c - > data/prevurb.gz
cut -c174-174 data.dat | gzip -c - > data/reasmig.gz
cut -c175-175 data.dat | gzip -c - > data/resurb.gz
rm data.dat
echo "  End vectorizing: " `date`
