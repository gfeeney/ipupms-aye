#!/bin/sh
# Vectorize data file
echo "Begin vectorizing: " `date`
gunzip -c source/ipumsi_00100.dat.gz > data.dat
sha1sum data.dat > source/ipumsi_00.sha1
mkdir data
cut -c1-3 data.dat | gzip -c - > data/cntry.gz
cut -c4-7 data.dat | gzip -c - > data/year.gz
cut -c8-11 data.dat | gzip -c - > data/sample.gz
cut -c12-21 data.dat | gzip -c - > data/serial.gz
cut -c22-28 data.dat | gzip -c - > data/dwnum.gz
cut -c29-29 data.dat | gzip -c - > data/hhnum.gz
cut -c30-30 data.dat | gzip -c - > data/hhn.gz
cut -c31-32 data.dat | gzip -c - > data/pernd.gz
cut -c33-34 data.dat | gzip -c - > data/pern.gz
cut -c35-35 data.dat | gzip -c - > data/fbig.gz
cut -c36-37 data.dat | gzip -c - > data/dept.gz
cut -c38-41 data.dat | gzip -c - > data/prov.gz
cut -c42-42 data.dat | gzip -c - > data/urban.gz
cut -c43-43 data.dat | gzip -c - > data/numhho.gz
cut -c44-45 data.dat | gzip -c - > data/typdwell.gz
cut -c46-46 data.dat | gzip -c - > data/occup.gz
cut -c47-47 data.dat | gzip -c - > data/wall.gz
cut -c48-48 data.dat | gzip -c - > data/floor.gz
cut -c49-49 data.dat | gzip -c - > data/watsrc.gz
cut -c50-50 data.dat | gzip -c - > data/watdaily.gz
cut -c51-52 data.dat | gzip -c - > data/wathrs.gz
cut -c53-53 data.dat | gzip -c - > data/watdays.gz
cut -c54-55 data.dat | gzip -c - > data/wathrs2.gz
cut -c56-56 data.dat | gzip -c - > data/sewage.gz
cut -c57-57 data.dat | gzip -c - > data/lighting.gz
cut -c58-59 data.dat | gzip -c - > data/rooms.gz
cut -c60-60 data.dat | gzip -c - > data/ownershp.gz
cut -c61-61 data.dat | gzip -c - > data/radio.gz
cut -c62-62 data.dat | gzip -c - > data/tv.gz
cut -c63-63 data.dat | gzip -c - > data/stereo.gz
cut -c64-64 data.dat | gzip -c - > data/washer.gz
cut -c65-65 data.dat | gzip -c - > data/fridge.gz
cut -c66-66 data.dat | gzip -c - > data/computr.gz
cut -c67-67 data.dat | gzip -c - > data/noappl.gz
cut -c68-68 data.dat | gzip -c - > data/phone.gz
cut -c69-69 data.dat | gzip -c - > data/cell.gz
cut -c70-70 data.dat | gzip -c - > data/internet.gz
cut -c71-71 data.dat | gzip -c - > data/cabletv.gz
cut -c72-72 data.dat | gzip -c - > data/noserv.gz
cut -c73-73 data.dat | gzip -c - > data/fuelcook.gz
cut -c74-74 data.dat | gzip -c - > data/chimney.gz
cut -c75-76 data.dat | gzip -c - > data/migrants.gz
cut -c77-77 data.dat | gzip -c - > data/disvis.gz
cut -c78-78 data.dat | gzip -c - > data/dishear.gz
cut -c79-79 data.dat | gzip -c - > data/disspk.gz
cut -c80-80 data.dat | gzip -c - > data/dislimb.gz
cut -c81-81 data.dat | gzip -c - > data/disother.gz
cut -c82-82 data.dat | gzip -c - > data/disnone.gz
cut -c83-85 data.dat | gzip -c - > data/pernum.gz
cut -c86-93 data.dat | gzip -c - > data/wtper.gz
cut -c94-95 data.dat | gzip -c - > data/pernum.gz
cut -c96-97 data.dat | gzip -c - > data/relate.gz
cut -c98-98 data.dat | gzip -c - > data/sex.gz
cut -c99-100 data.dat | gzip -c - > data/age.gz
cut -c101-102 data.dat | gzip -c - > data/agemonth.gz
cut -c103-103 data.dat | gzip -c - > data/birthreg.gz
cut -c104-104 data.dat | gzip -c - > data/resident.gz
cut -c105-108 data.dat | gzip -c - > data/resctry.gz
cut -c109-112 data.dat | gzip -c - > data/resprov.gz
cut -c113-113 data.dat | gzip -c - > data/here5yr.gz
cut -c114-117 data.dat | gzip -c - > data/ctry5yr.gz
cut -c118-119 data.dat | gzip -c - > data/dept5yr.gz
cut -c120-123 data.dat | gzip -c - > data/prov5yr.gz
cut -c124-124 data.dat | gzip -c - > data/bplhere.gz
cut -c125-128 data.dat | gzip -c - > data/bplctry.gz
cut -c129-132 data.dat | gzip -c - > data/bplprov.gz
cut -c133-133 data.dat | gzip -c - > data/insursis.gz
cut -c134-134 data.dat | gzip -c - > data/insuress.gz
cut -c135-135 data.dat | gzip -c - > data/insuroth.gz
cut -c136-136 data.dat | gzip -c - > data/insurnon.gz
cut -c137-137 data.dat | gzip -c - > data/mtong.gz
cut -c138-138 data.dat | gzip -c - > data/lit.gz
cut -c139-139 data.dat | gzip -c - > data/edlevel.gz
cut -c140-140 data.dat | gzip -c - > data/edgrade.gz
cut -c141-141 data.dat | gzip -c - > data/edyear.gz
cut -c142-144 data.dat | gzip -c - > data/edattain.gz
cut -c145-145 data.dat | gzip -c - > data/schoolatt.gz
cut -c146-146 data.dat | gzip -c - > data/worked.gz
cut -c147-147 data.dat | gzip -c - > data/reasonwk.gz
cut -c148-149 data.dat | gzip -c - > data/lastnowk.gz
cut -c150-153 data.dat | gzip -c - > data/ind.gz
cut -c154-155 data.dat | gzip -c - > data/ind2.gz
cut -c156-156 data.dat | gzip -c - > data/classwkr.gz
cut -c157-157 data.dat | gzip -c - > data/empsize.gz
cut -c158-158 data.dat | gzip -c - > data/religion.gz
cut -c159-159 data.dat | gzip -c - > data/marst.gz
cut -c160-161 data.dat | gzip -c - > data/chborn.gz
cut -c162-163 data.dat | gzip -c - > data/chsurv.gz
cut -c164-165 data.dat | gzip -c - > data/lstbthmo.gz
cut -c166-169 data.dat | gzip -c - > data/lstbthyr.gz
cut -c170-171 data.dat | gzip -c - > data/agebirth.gz
cut -c172-172 data.dat | gzip -c - > data/idcard.gz
rm data.dat
echo "  End vectorizing: " `date`
