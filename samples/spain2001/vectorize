cut -c151-152 data.dat | gzip -c - > data/prevprov.gz
cut -c153-157 data.dat | gzip -c - > data/prevmun.gz
cut -c158-158 data.dat | gzip -c - > data/res10.gz
cut -c159-160 data.dat | gzip -c - > data/res10prv.gz
cut -c161-161 data.dat | gzip -c - > data/indlang.gz
cut -c162-163 data.dat | gzip -c - > data/emp2.gz
cut -c164-165 data.dat | gzip -c - > data/emp3.gz
cut -c166-166 data.dat | gzip -c - > data/workpl.gz
cut -c167-168 data.dat | gzip -c - > data/wkprov.gz
cut -c169-169 data.dat | gzip -c - > data/trips.gz
cut -c170-171 data.dat | gzip -c - > data/trans1.gz
cut -c172-173 data.dat | gzip -c - > data/trans2.gz
cut -c174-174 data.dat | gzip -c - > data/travtime.gz
cut -c175-176 data.dat | gzip -c - > data/study1.gz
cut -c177-178 data.dat | gzip -c - > data/study2.gz
cut -c179-180 data.dat | gzip -c - > data/study3.gz
cut -c181-182 data.dat | gzip -c - > data/occ.gz
cut -c183-183 data.dat | gzip -c - > data/classwk.gz
cut -c184-185 data.dat | gzip -c - > data/ind.gz
cut -c186-187 data.dat | gzip -c - > data/socecon.gz
cut -c188-189 data.dat | gzip -c - > data/wkhrs.gz
cut -c190-190 data.dat | gzip -c - > data/famnum.gz
