#!/bin/sh
# Vectorize data file
echo "Begin vectorizing: " `date`
gunzip -c source/ipumsi_00112.dat.gz > data.dat
sha1sum data.dat > source/ipumsi_00.sha1
mkdir data
cut -c1-3 data.dat | gzip -c - > data/cntry.gz
cut -c4-7 data.dat | gzip -c - > data/year.gz
cut -c8-11 data.dat | gzip -c - > data/sample.gz
cut -c12-21 data.dat | gzip -c - > data/serial.gz
cut -c22-27 data.dat | gzip -c - > data/dwnum.gz
cut -c28-29 data.dat | gzip -c - > data/pern.gz
cut -c30-30 data.dat | gzip -c - > data/fbig.gz
cut -c31-32 data.dat | gzip -c - > data/auton.gz
cut -c33-34 data.dat | gzip -c - > data/prov.gz
cut -c35-35 data.dat | gzip -c - > data/collect.gz
cut -c36-36 data.dat | gzip -c - > data/dwtype.gz
cut -c37-38 data.dat | gzip -c - > data/colltyp.gz
cut -c39-42 data.dat | gzip -c - > data/yrmoved.gz
cut -c43-43 data.dat | gzip -c - > data/owner.gz
cut -c44-44 data.dat | gzip -c - > data/noisy.gz
cut -c45-45 data.dat | gzip -c - > data/pollute.gz
cut -c46-46 data.dat | gzip -c - > data/clean.gz
cut -c47-47 data.dat | gzip -c - > data/commun.gz
cut -c48-48 data.dat | gzip -c - > data/green.gz
cut -c49-49 data.dat | gzip -c - > data/delinq.gz
cut -c50-50 data.dat | gzip -c - > data/noserv.gz
cut -c51-51 data.dat | gzip -c - > data/aircon.gz
cut -c52-52 data.dat | gzip -c - > data/heat.gz
cut -c53-53 data.dat | gzip -c - > data/heatfuel.gz
cut -c54-55 data.dat | gzip -c - > data/stories.gz
cut -c56-57 data.dat | gzip -c - > data/rooms.gz
cut -c58-60 data.dat | gzip -c - > data/space.gz
cut -c61-61 data.dat | gzip -c - > data/secdw.gz
cut -c62-62 data.dat | gzip -c - > data/secdwl.gz
cut -c63-64 data.dat | gzip -c - > data/secdwp.gz
cut -c65-67 data.dat | gzip -c - > data/secdwu.gz
cut -c68-68 data.dat | gzip -c - > data/vehic.gz
cut -c69-70 data.dat | gzip -c - > data/flrup.gz
cut -c71-71 data.dat | gzip -c - > data/flrdwn.gz
cut -c72-72 data.dat | gzip -c - > data/bldtype.gz
cut -c73-74 data.dat | gzip -c - > data/constyr1.gz
cut -c75-78 data.dat | gzip -c - > data/constyr2.gz
cut -c79-79 data.dat | gzip -c - > data/bldcond.gz
cut -c80-80 data.dat | gzip -c - > data/ownertyp.gz
cut -c81-81 data.dat | gzip -c - > data/access.gz
cut -c82-82 data.dat | gzip -c - > data/runwat.gz
cut -c83-83 data.dat | gzip -c - > data/sewage.gz
cut -c84-84 data.dat | gzip -c - > data/doorkeep.gz
cut -c85-85 data.dat | gzip -c - > data/garage.gz
cut -c86-86 data.dat | gzip -c - > data/gas.gz
cut -c87-87 data.dat | gzip -c - > data/phone.gz
cut -c88-88 data.dat | gzip -c - > data/hotwat.gz
cut -c89-89 data.dat | gzip -c - > data/nfam.gz
cut -c90-90 data.dat | gzip -c - > data/nnuc.gz
cut -c91-92 data.dat | gzip -c - > data/nperfam.gz
cut -c93-95 data.dat | gzip -c - > data/pernum.gz
cut -c96-103 data.dat | gzip -c - > data/wtper.gz
cut -c104-105 data.dat | gzip -c - > data/pernum.gz
cut -c106-107 data.dat | gzip -c - > data/pernumo.gz
cut -c108-109 data.dat | gzip -c - > data/birmo.gz
cut -c110-113 data.dat | gzip -c - > data/biryr.gz
cut -c114-116 data.dat | gzip -c - > data/age.gz
cut -c117-117 data.dat | gzip -c - > data/sex.gz
cut -c118-120 data.dat | gzip -c - > data/nation.gz
cut -c121-122 data.dat | gzip -c - > data/bplauton.gz
cut -c123-124 data.dat | gzip -c - > data/bplpr.gz
cut -c125-129 data.dat | gzip -c - > data/bplmun.gz
cut -c130-130 data.dat | gzip -c - > data/marst.gz
