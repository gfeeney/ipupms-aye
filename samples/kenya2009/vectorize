cut -c30-30 data.dat | gzip -c - > data/fbig.gz
cut -c31-31 data.dat | gzip -c - > data/prov.gz
cut -c32-34 data.dat | gzip -c - > data/dist.gz
cut -c35-35 data.dat | gzip -c - > data/urban.gz
cut -c36-36 data.dat | gzip -c - > data/areastat.gz
cut -c37-39 data.dat | gzip -c - > data/constit.gz
cut -c40-40 data.dat | gzip -c - > data/hhtype.gz
cut -c41-43 data.dat | gzip -c - > data/county.gz
cut -c44-45 data.dat | gzip -c - > data/bthyrhh.gz
cut -c46-47 data.dat | gzip -c - > data/dthyrhh.gz
cut -c48-49 data.dat | gzip -c - > data/excattle.gz
