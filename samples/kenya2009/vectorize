cut -c96-96 data.dat | gzip -c - > data/cellphon.gz
cut -c97-97 data.dat | gzip -c - > data/phone.gz
cut -c98-98 data.dat | gzip -c - > data/computr.gz
cut -c99-99 data.dat | gzip -c - > data/bike.gz
cut -c100-100 data.dat | gzip -c - > data/motorcyc.gz
cut -c101-101 data.dat | gzip -c - > data/auto.gz
cut -c102-102 data.dat | gzip -c - > data/truck.gz
