cut -c75-76 data.dat | gzip -c - > data/othanim.gz
cut -c77-78 data.dat | gzip -c - > data/dwunits.gz
cut -c79-80 data.dat | gzip -c - > data/rooms.gz
cut -c81-82 data.dat | gzip -c - > data/tenure.gz
cut -c83-84 data.dat | gzip -c - > data/roof.gz
