cut -c171-172 data.dat | gzip -c - > data/cntry05.gz
cut -c173-174 data.dat | gzip -c - > data/prov05.gz
cut -c175-178 data.dat | gzip -c - > data/comm05.gz
cut -c179-179 data.dat | gzip -c - > data/abroad.gz
cut -c180-181 data.dat | gzip -c - > data/resdur.gz
cut -c182-182 data.dat | gzip -c - > data/whyret.gz
cut -c183-183 data.dat | gzip -c - > data/disab.gz
cut -c184-184 data.dat | gzip -c - > data/religion.gz
cut -c185-186 data.dat | gzip -c - > data/nation.gz
cut -c187-187 data.dat | gzip -c - > data/school.gz
cut -c188-189 data.dat | gzip -c - > data/edlevel.gz
cut -c190-191 data.dat | gzip -c - > data/litflang.gz
cut -c192-193 data.dat | gzip -c - > data/litnat.gz
cut -c194-195 data.dat | gzip -c - > data/lang.gz
cut -c196-196 data.dat | gzip -c - > data/empstat.gz
cut -c197-197 data.dat | gzip -c - > data/classwk.gz
cut -c198-198 data.dat | gzip -c - > data/marst.gz
cut -c199-200 data.dat | gzip -c - > data/chbornm.gz
cut -c201-202 data.dat | gzip -c - > data/chbornf.gz
cut -c203-204 data.dat | gzip -c - > data/chsurvm.gz
cut -c205-206 data.dat | gzip -c - > data/chsurvf.gz
cut -c207-207 data.dat | gzip -c - > data/bthlyrm.gz
