#!/bin/sh
# Vectorize data file
echo "Begin vectorizing: " `date`
gunzip -c source/ipumsi_00056.dat.gz > data.dat
sha1sum data.dat > source/ipumsi_0005.sha1
mkdir data
cut -c1-3 data.dat | gzip -c - > data/cntry.gz
cut -c4-7 data.dat | gzip -c - > data/year.gz
cut -c8-11 data.dat | gzip -c - > data/sample.gz
cut -c12-21 data.dat | gzip -c - > data/serial.gz
cut -c22-28 data.dat | gzip -c - > data/dwnum.gz
cut -c29-30 data.dat | gzip -c - > data/pern.gz
cut -c31-31 data.dat | gzip -c - > data/fbig.gz
cut -c32-33 data.dat | gzip -c - > data/region.gz
cut -c34-35 data.dat | gzip -c - > data/prov.gz
cut -c36-36 data.dat | gzip -c - > data/urban.gz
cut -c37-38 data.dat | gzip -c - > data/numhhqs.gz
cut -c39-39 data.dat | gzip -c - > data/hhzone.gz
cut -c40-40 data.dat | gzip -c - > data/hhtype.gz
cut -c41-41 data.dat | gzip -c - > data/occup.gz
cut -c42-43 data.dat | gzip -c - > data/rooms.gz
cut -c44-44 data.dat | gzip -c - > data/walls.gz
cut -c45-45 data.dat | gzip -c - > data/roof.gz
cut -c46-46 data.dat | gzip -c - > data/floor.gz
cut -c47-47 data.dat | gzip -c - > data/light.gz
cut -c48-48 data.dat | gzip -c - > data/fuelck.gz
cut -c49-49 data.dat | gzip -c - > data/watsup.gz
cut -c50-50 data.dat | gzip -c - > data/toilet.gz
cut -c51-51 data.dat | gzip -c - > data/trash.gz
cut -c52-52 data.dat | gzip -c - > data/sewer.gz
cut -c53-54 data.dat | gzip -c - > data/radio.gz
cut -c55-56 data.dat | gzip -c - > data/tv.gz
cut -c57-58 data.dat | gzip -c - > data/phone.gz
cut -c59-60 data.dat | gzip -c - > data/cellphon.gz
cut -c61-62 data.dat | gzip -c - > data/refrig.gz
cut -c63-64 data.dat | gzip -c - > data/computer.gz
cut -c65-66 data.dat | gzip -c - > data/internet.gz
cut -c67-68 data.dat | gzip -c - > data/bike.gz
cut -c69-70 data.dat | gzip -c - > data/motorbik.gz
cut -c71-72 data.dat | gzip -c - > data/auto.gz
cut -c73-74 data.dat | gzip -c - > data/boat.gz
cut -c75-76 data.dat | gzip -c - > data/cart.gz
cut -c77-78 data.dat | gzip -c - > data/camel.gz
cut -c79-80 data.dat | gzip -c - > data/horse.gz
cut -c81-82 data.dat | gzip -c - > data/donkey.gz
cut -c83-85 data.dat | gzip -c - > data/respresm.gz
cut -c86-88 data.dat | gzip -c - > data/resabsm.gz
cut -c89-91 data.dat | gzip -c - > data/pamales.gz
cut -c92-94 data.dat | gzip -c - > data/visitmal.gz
cut -c95-97 data.dat | gzip -c - > data/totmale.gz
cut -c98-100 data.dat | gzip -c - > data/emigmal.gz
cut -c101-103 data.dat | gzip -c - > data/respresf.gz
cut -c104-106 data.dat | gzip -c - > data/resabsf.gz
cut -c107-109 data.dat | gzip -c - > data/pafem.gz
cut -c110-112 data.dat | gzip -c - > data/visfem.gz
cut -c113-115 data.dat | gzip -c - > data/totfem.gz
cut -c116-118 data.dat | gzip -c - > data/emigfem.gz
cut -c119-121 data.dat | gzip -c - > data/respres.gz
cut -c122-124 data.dat | gzip -c - > data/resabs.gz
cut -c125-127 data.dat | gzip -c - > data/presabs.gz
cut -c128-130 data.dat | gzip -c - > data/visitors.gz
cut -c131-133 data.dat | gzip -c - > data/emigtot.gz
cut -c134-134 data.dat | gzip -c - > data/lsquint.gz
cut -c135-137 data.dat | gzip -c - > data/pernum.gz
cut -c138-145 data.dat | gzip -c - > data/wtper.gz
cut -c146-146 data.dat | gzip -c - > data/resident.gz
cut -c147-148 data.dat | gzip -c - > data/pernum.gz
cut -c149-149 data.dat | gzip -c - > data/sex.gz
cut -c150-151 data.dat | gzip -c - > data/relate.gz
cut -c152-152 data.dat | gzip -c - > data/status.gz
