cut -c146-146 data.dat | gzip -c - > data/ethnic.gz
cut -c147-147 data.dat | gzip -c - > data/indig.gz
cut -c148-148 data.dat | gzip -c - > data/indigsp.gz
cut -c149-149 data.dat | gzip -c - > data/lang.gz
cut -c150-150 data.dat | gzip -c - > data/socsec.gz
cut -c151-151 data.dat | gzip -c - > data/disab.gz
cut -c152-152 data.dat | gzip -c - > data/res5plc.gz
cut -c153-155 data.dat | gzip -c - > data/res5cant.gz
cut -c156-156 data.dat | gzip -c - > data/school.gz
cut -c157-158 data.dat | gzip -c - > data/grade.gz
cut -c159-159 data.dat | gzip -c - > data/edattain.gz
cut -c160-160 data.dat | gzip -c - > data/lit.gz
cut -c161-161 data.dat | gzip -c - > data/marst.gz
cut -c162-162 data.dat | gzip -c - > data/empstat.gz
cut -c163-163 data.dat | gzip -c - > data/sector.gz
cut -c164-165 data.dat | gzip -c - > data/ind2.gz
cut -c166-168 data.dat | gzip -c - > data/ind3.gz
cut -c169-169 data.dat | gzip -c - > data/occ1.gz
cut -c170-171 data.dat | gzip -c - > data/occ2.gz
cut -c172-174 data.dat | gzip -c - > data/occ3.gz
cut -c175-178 data.dat | gzip -c - > data/occ4.gz
cut -c179-179 data.dat | gzip -c - > data/classwk.gz
cut -c180-181 data.dat | gzip -c - > data/chborn.gz
cut -c182-183 data.dat | gzip -c - > data/chsurv.gz
cut -c184-185 data.dat | gzip -c - > data/hhtypep.gz
