#!/bin/sh
# Vectorize data file
echo "Begin vectorizing: " `date`
gunzip -c source/ipumsi_00064.dat.gz > data.dat
sha1sum data.dat > source/ipumsi_00.sha1
mkdir data
cut -c1-3 data.dat | gzip -c - > data/cntry.gz
cut -c4-7 data.dat | gzip -c - > data/year.gz
cut -c8-11 data.dat | gzip -c - > data/sample.gz
cut -c12-21 data.dat | gzip -c - > data/serial.gz
cut -c22-24 data.dat | gzip -c - > data/persons.gz
cut -c25-32 data.dat | gzip -c - > data/wthh.gz
cut -c33-34 data.dat | gzip -c - > data/gq.gz
cut -c35-35 data.dat | gzip -c - > data/unrel.gz
cut -c36-37 data.dat | gzip -c - > data/regionw.gz
cut -c38-43 data.dat | gzip -c - > data/geolev1.gz
cut -c44-49 data.dat | gzip -c - > data/geo1a_cr.gz
cut -c50-52 data.dat | gzip -c - > data/geo2b_cr.gz
cut -c53-59 data.dat | gzip -c - > data/dwnum.gz
cut -c60-60 data.dat | gzip -c - > data/hhnum.gz
cut -c61-61 data.dat | gzip -c - > data/hhn.gz
cut -c62-63 data.dat | gzip -c - > data/pernd.gz
cut -c64-65 data.dat | gzip -c - > data/pern.gz
cut -c66-66 data.dat | gzip -c - > data/fbig.gz
cut -c67-67 data.dat | gzip -c - > data/prov.gz
cut -c68-68 data.dat | gzip -c - > data/region.gz
cut -c69-69 data.dat | gzip -c - > data/zone.gz
cut -c70-70 data.dat | gzip -c - > data/indters.gz
cut -c71-71 data.dat | gzip -c - > data/dwtype.gz
cut -c72-72 data.dat | gzip -c - > data/occup.gz
cut -c73-79 data.dat | gzip -c - > data/rentmort.gz
cut -c80-80 data.dat | gzip -c - > data/wall.gz
cut -c81-81 data.dat | gzip -c - > data/roof.gz
cut -c82-82 data.dat | gzip -c - > data/ceiling.gz
cut -c83-83 data.dat | gzip -c - > data/floor.gz
cut -c84-84 data.dat | gzip -c - > data/wallc.gz
cut -c85-85 data.dat | gzip -c - > data/roofc.gz
cut -c86-86 data.dat | gzip -c - > data/floorc.gz
cut -c87-88 data.dat | gzip -c - > data/bedrm.gz
cut -c89-90 data.dat | gzip -c - > data/rm.gz
cut -c91-91 data.dat | gzip -c - > data/watsup.gz
cut -c92-92 data.dat | gzip -c - > data/watsrc.gz
cut -c93-93 data.dat | gzip -c - > data/sewer.gz
cut -c94-94 data.dat | gzip -c - > data/toilet.gz
cut -c95-95 data.dat | gzip -c - > data/elect.gz
cut -c96-96 data.dat | gzip -c - > data/fuelcook.gz
cut -c97-97 data.dat | gzip -c - > data/tv.gz
cut -c98-98 data.dat | gzip -c - > data/phone.gz
cut -c99-99 data.dat | gzip -c - > data/refrig.gz
cut -c100-100 data.dat | gzip -c - > data/micro.gz
cut -c101-101 data.dat | gzip -c - > data/shower.gz
cut -c102-102 data.dat | gzip -c - > data/watheat.gz
cut -c103-103 data.dat | gzip -c - > data/washer.gz
cut -c104-104 data.dat | gzip -c - > data/computr.gz
cut -c105-105 data.dat | gzip -c - > data/car.gz
cut -c106-106 data.dat | gzip -c - > data/dwcond1.gz
cut -c107-107 data.dat | gzip -c - > data/dwcond2.gz
cut -c108-108 data.dat | gzip -c - > data/appown.gz
cut -c109-109 data.dat | gzip -c - > data/crowd.gz
cut -c110-111 data.dat | gzip -c - > data/hhtype.gz
cut -c112-112 data.dat | gzip -c - > data/nbishel.gz
cut -c113-113 data.dat | gzip -c - > data/nbihyg.gz
cut -c114-114 data.dat | gzip -c - > data/nbiknow.gz
cut -c115-115 data.dat | gzip -c - > data/nbicons.gz
cut -c116-116 data.dat | gzip -c - > data/nbibas.gz
cut -c117-119 data.dat | gzip -c - > data/pernum.gz
cut -c120-127 data.dat | gzip -c - > data/wtper.gz
cut -c128-129 data.dat | gzip -c - > data/pernum.gz
cut -c130-130 data.dat | gzip -c - > data/relate.gz
