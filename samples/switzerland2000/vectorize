#!/bin/sh
# Vectorize data file
echo "Begin vectorizing: " `date`
gunzip -c source/ipumsi_00114.dat.gz > data.dat
sha1sum data.dat > source/ipumsi_00.sha1
mkdir data
cut -c1-3 data.dat | gzip -c - > data/cntry.gz
cut -c4-7 data.dat | gzip -c - > data/year.gz
cut -c8-11 data.dat | gzip -c - > data/sample.gz
cut -c12-21 data.dat | gzip -c - > data/serial.gz
cut -c22-23 data.dat | gzip -c - > data/pern.gz
cut -c24-24 data.dat | gzip -c - > data/fbig.gz
cut -c25-27 data.dat | gzip -c - > data/fbig_nd.gz
cut -c28-30 data.dat | gzip -c - > data/fbig_nh.gz
cut -c31-32 data.dat | gzip -c - > data/canton.gz
cut -c33-36 data.dat | gzip -c - > data/hhtype.gz
cut -c37-37 data.dat | gzip -c - > data/categbld.gz
cut -c38-38 data.dat | gzip -c - > data/typebldg.gz
cut -c39-39 data.dat | gzip -c - > data/yrbuilt.gz
cut -c40-41 data.dat | gzip -c - > data/floors.gz
cut -c42-42 data.dat | gzip -c - > data/ownbldg.gz
cut -c43-43 data.dat | gzip -c - > data/heating.gz
cut -c44-44 data.dat | gzip -c - > data/hotwater.gz
cut -c45-45 data.dat | gzip -c - > data/fuelheat.gz
cut -c46-46 data.dat | gzip -c - > data/fuelwatr.gz
cut -c47-48 data.dat | gzip -c - > data/dwellbld.gz
cut -c49-50 data.dat | gzip -c - > data/permdwls.gz
cut -c51-52 data.dat | gzip -c - > data/tempdwls.gz
cut -c53-54 data.dat | gzip -c - > data/vacdwls.gz
cut -c55-56 data.dat | gzip -c - > data/rooms.gz
cut -c57-58 data.dat | gzip -c - > data/roomscon.gz
cut -c59-62 data.dat | gzip -c - > data/area.gz
cut -c63-63 data.dat | gzip -c - > data/kitchen.gz
cut -c64-64 data.dat | gzip -c - > data/occstatus.gz
cut -c65-69 data.dat | gzip -c - > data/rent.gz
cut -c70-72 data.dat | gzip -c - > data/pernum.gz
cut -c73-80 data.dat | gzip -c - > data/wtper.gz
cut -c81-82 data.dat | gzip -c - > data/pernum.gz
cut -c83-84 data.dat | gzip -c - > data/labforce.gz
cut -c85-88 data.dat | gzip -c - > data/nation.gz
cut -c89-89 data.dat | gzip -c - > data/res5yr.gz
cut -c90-90 data.dat | gzip -c - > data/bpl.gz
cut -c91-91 data.dat | gzip -c - > data/citiz.gz
cut -c92-92 data.dat | gzip -c - > data/placwork.gz
cut -c93-93 data.dat | gzip -c - > data/placeduc.gz
cut -c94-95 data.dat | gzip -c - > data/age.gz
cut -c96-96 data.dat | gzip -c - > data/sex.gz
cut -c97-97 data.dat | gzip -c - > data/marst.gz
cut -c98-100 data.dat | gzip -c - > data/relig.gz
cut -c101-103 data.dat | gzip -c - > data/mtongue.gz
cut -c104-104 data.dat | gzip -c - > data/head.gz
cut -c105-105 data.dat | gzip -c - > data/relate.gz
cut -c106-110 data.dat | gzip -c - > data/edprof.gz
cut -c111-115 data.dat | gzip -c - > data/occ.gz
cut -c116-118 data.dat | gzip -c - > data/isco3.gz
cut -c119-122 data.dat | gzip -c - > data/isco4.gz
cut -c123-123 data.dat | gzip -c - > data/classwk.gz
cut -c124-124 data.dat | gzip -c - > data/hrswork.gz
cut -c125-127 data.dat | gzip -c - > data/ind.gz
cut -c128-128 data.dat | gzip -c - > data/bustype.gz
cut -c129-131 data.dat | gzip -c - > data/socecon.gz
cut -c132-132 data.dat | gzip -c - > data/worktime.gz
cut -c133-133 data.dat | gzip -c - > data/wrktrips.gz
cut -c134-135 data.dat | gzip -c - > data/wtravmod.gz
cut -c136-136 data.dat | gzip -c - > data/schltime.gz
cut -c137-137 data.dat | gzip -c - > data/schtrips.gz
cut -c138-139 data.dat | gzip -c - > data/stravmod.gz
cut -c140-140 data.dat | gzip -c - > data/edatt1.gz
cut -c141-142 data.dat | gzip -c - > data/edatt2.gz
cut -c143-146 data.dat | gzip -c - > data/yrmarst.gz
