cut -c163-166 data.dat | gzip -c - > data/yrbirth2.gz
cut -c167-170 data.dat | gzip -c - > data/yrbirth3.gz
cut -c171-174 data.dat | gzip -c - > data/yrbirth4.gz
cut -c175-178 data.dat | gzip -c - > data/yrlstbth.gz
cut -c179-182 data.dat | gzip -c - > data/yrbthyng.gz
cut -c183-186 data.dat | gzip -c - > data/langhmwk.gz
cut -c187-189 data.dat | gzip -c - > data/langhome.gz
cut -c190-192 data.dat | gzip -c - > data/langwksc.gz
cut -c193-194 data.dat | gzip -c - > data/edattend.gz
cut -c195-197 data.dat | gzip -c - > data/empstat.gz
cut -c198-199 data.dat | gzip -c - > data/multactv.gz
cut -c200-200 data.dat | gzip -c - > data/homecare.gz
cut -c201-201 data.dat | gzip -c - > data/voluntry.gz
cut -c202-204 data.dat | gzip -c - > data/hrsfull.gz
cut -c205-206 data.dat | gzip -c - > data/hrspart.gz
cut -c207-208 data.dat | gzip -c - > data/hrseduc.gz
cut -c209-211 data.dat | gzip -c - > data/hrshomwk.gz
cut -c212-213 data.dat | gzip -c - > data/hrsvolun.gz
cut -c214-214 data.dat | gzip -c - > data/commwork.gz
