#!/bin/sh
# Vectorize data file
echo "Begin vectorizing: " `date`
gunzip -c source/ipumsi_00060.dat.gz > data.dat
sha1sum data.dat > source/ipumsi_0006.sha1
mkdir data
cut -c1-3 data.dat | gzip -c - > data/cntry.gz
cut -c4-7 data.dat | gzip -c - > data/year.gz
cut -c8-11 data.dat | gzip -c - > data/sample.gz
cut -c12-21 data.dat | gzip -c - > data/serial.gz
cut -c22-22 data.dat | gzip -c - > data/rectype.gz
cut -c23-29 data.dat | gzip -c - > data/dwnum.gz
cut -c30-31 data.dat | gzip -c - > data/pern.gz
cut -c32-33 data.dat | gzip -c - > data/province.gz
cut -c34-36 data.dat | gzip -c - > data/district.gz
cut -c37-37 data.dat | gzip -c - > data/urban.gz
cut -c38-38 data.dat | gzip -c - > data/urbaniz.gz
cut -c39-39 data.dat | gzip -c - > data/hhtype.gz
cut -c40-41 data.dat | gzip -c - > data/hhsize.gz
cut -c42-43 data.dat | gzip -c - > data/nmale.gz
cut -c44-45 data.dat | gzip -c - > data/nfemale.gz
cut -c46-46 data.dat | gzip -c - > data/hasdwell.gz
cut -c47-47 data.dat | gzip -c - > data/shareh.gz
cut -c48-48 data.dat | gzip -c - > data/rephouse.gz
cut -c49-49 data.dat | gzip -c - > data/dwtype.gz
cut -c50-51 data.dat | gzip -c - > data/rooms.gz
cut -c52-53 data.dat | gzip -c - > data/bedroom.gz
cut -c54-57 data.dat | gzip -c - > data/flrspace.gz
cut -c58-58 data.dat | gzip -c - > data/pier.gz
cut -c59-59 data.dat | gzip -c - > data/roof.gz
cut -c60-60 data.dat | gzip -c - > data/wall.gz
cut -c61-61 data.dat | gzip -c - > data/useyear.gz
cut -c62-63 data.dat | gzip -c - > data/beginyr.gz
cut -c64-64 data.dat | gzip -c - > data/ownershp.gz
cut -c65-65 data.dat | gzip -c - > data/light.gz
cut -c66-66 data.dat | gzip -c - > data/fuelcook.gz
cut -c67-68 data.dat | gzip -c - > data/watsrc.gz
cut -c69-69 data.dat | gzip -c - > data/toilet.gz
cut -c70-70 data.dat | gzip -c - > data/tv.gz
cut -c71-71 data.dat | gzip -c - > data/radio.gz
cut -c72-72 data.dat | gzip -c - > data/phone.gz
cut -c73-73 data.dat | gzip -c - > data/computr.gz
cut -c74-74 data.dat | gzip -c - > data/washer.gz
cut -c75-75 data.dat | gzip -c - > data/refrig.gz
cut -c76-76 data.dat | gzip -c - > data/aircon.gz
cut -c77-77 data.dat | gzip -c - > data/moto.gz
cut -c78-80 data.dat | gzip -c - > data/pernum.gz
cut -c81-88 data.dat | gzip -c - > data/wtper.gz
cut -c89-90 data.dat | gzip -c - > data/pernum.gz
cut -c91-92 data.dat | gzip -c - > data/personno.gz
cut -c93-93 data.dat | gzip -c - > data/relate.gz
cut -c94-94 data.dat | gzip -c - > data/sex.gz
cut -c95-96 data.dat | gzip -c - > data/age.gz
cut -c97-97 data.dat | gzip -c - > data/mig.gz
cut -c98-98 data.dat | gzip -c - > data/urbmig.gz
cut -c99-99 data.dat | gzip -c - > data/educ.gz
cut -c100-101 data.dat | gzip -c - > data/edattan.gz
cut -c102-103 data.dat | gzip -c - > data/grade.gz
cut -c104-107 data.dat | gzip -c - > data/edattain.gz
cut -c108-108 data.dat | gzip -c - > data/lit.gz
cut -c109-109 data.dat | gzip -c - > data/marst.gz
cut -c110-111 data.dat | gzip -c - > data/qualif.gz
cut -c112-112 data.dat | gzip -c - > data/emp1.gz
cut -c113-113 data.dat | gzip -c - > data/emp2.gz
cut -c114-114 data.dat | gzip -c - > data/emp3.gz
cut -c115-117 data.dat | gzip -c - > data/occ.gz
cut -c118-118 data.dat | gzip -c - > data/classwk.gz
cut -c119-119 data.dat | gzip -c - > data/sector.gz
cut -c120-122 data.dat | gzip -c - > data/ind.gz
cut -c123-123 data.dat | gzip -c - > data/unemp.gz
cut -c124-124 data.dat | gzip -c - > data/unemp1.gz
cut -c125-125 data.dat | gzip -c - > data/reasue.gz
cut -c126-126 data.dat | gzip -c - > data/birthx.gz
cut -c127-127 data.dat | gzip -c - > data/disvis.gz
cut -c128-128 data.dat | gzip -c - > data/dishear.gz
cut -c129-129 data.dat | gzip -c - > data/diswalk.gz
cut -c130-130 data.dat | gzip -c - > data/disrem.gz
cut -c131-132 data.dat | gzip -c - > data/nchhh.gz
cut -c133-134 data.dat | gzip -c - > data/nchelse.gz
cut -c135-136 data.dat | gzip -c - > data/nchdead.gz
cut -c137-140 data.dat | gzip -c - > data/lstbyr.gz
cut -c141-142 data.dat | gzip -c - > data/lstbmo.gz
cut -c143-143 data.dat | gzip -c - > data/lstbfem.gz
cut -c144-144 data.dat | gzip -c - > data/lstbmale.gz
cut -c145-146 data.dat | gzip -c - > data/month.gz
cut -c147-150 data.dat | gzip -c - > data/birthyr.gz
cut -c151-151 data.dat | gzip -c - > data/ethnic.gz
cut -c152-152 data.dat | gzip -c - > data/relig.gz
cut -c153-155 data.dat | gzip -c - > data/migdist.gz
cut -c156-157 data.dat | gzip -c - > data/migprov.gz
rm data.dat
echo "  End vectorizing: " `date`
