#!/bin/sh
# Vectorize data file
echo "Begin vectorizing: " `date`
gunzip -c source/ipumsi_00060.dat.gz > data.dat
sha1sum data.dat > source/ipumsi_00.sha1
mkdir data
cut -c1-3 data.dat | gzip -c - > data/cntry.gz
cut -c4-7 data.dat | gzip -c - > data/year.gz
cut -c8-11 data.dat | gzip -c - > data/sample.gz
cut -c12-21 data.dat | gzip -c - > data/serial.gz
cut -c22-22 data.dat | gzip -c - > data/vn09rectype.gz
cut -c23-29 data.dat | gzip -c - > data/vn09dwnum.gz
cut -c30-31 data.dat | gzip -c - > data/vn09pern.gz
cut -c32-33 data.dat | gzip -c - > data/vn09province.gz
cut -c34-36 data.dat | gzip -c - > data/vn09district.gz
cut -c37-37 data.dat | gzip -c - > data/vn09urban.gz
cut -c38-38 data.dat | gzip -c - > data/vn09urbaniz.gz
cut -c39-39 data.dat | gzip -c - > data/vn09hhtype.gz
cut -c40-41 data.dat | gzip -c - > data/vn09hhsize.gz
cut -c42-43 data.dat | gzip -c - > data/vn09nmale.gz
cut -c44-45 data.dat | gzip -c - > data/vn09nfemale.gz
cut -c46-46 data.dat | gzip -c - > data/vn09hasdwell.gz
cut -c47-47 data.dat | gzip -c - > data/vn09shareh.gz
cut -c48-48 data.dat | gzip -c - > data/vn09rephouse.gz
cut -c49-49 data.dat | gzip -c - > data/vn09dwtype.gz
cut -c50-51 data.dat | gzip -c - > data/vn09rooms.gz
cut -c52-53 data.dat | gzip -c - > data/vn09bedroom.gz
cut -c54-57 data.dat | gzip -c - > data/vn09flrspace.gz
cut -c58-58 data.dat | gzip -c - > data/vn09pier.gz
cut -c59-59 data.dat | gzip -c - > data/vn09roof.gz
cut -c60-60 data.dat | gzip -c - > data/vn09wall.gz
cut -c61-61 data.dat | gzip -c - > data/vn09useyear.gz
cut -c62-63 data.dat | gzip -c - > data/vn09beginyr.gz
cut -c64-64 data.dat | gzip -c - > data/vn09ownershp.gz
cut -c65-65 data.dat | gzip -c - > data/vn09light.gz
cut -c66-66 data.dat | gzip -c - > data/vn09fuelcook.gz
cut -c67-68 data.dat | gzip -c - > data/vn09watsrc.gz
cut -c69-69 data.dat | gzip -c - > data/vn09toilet.gz
cut -c70-70 data.dat | gzip -c - > data/vn09tv.gz
cut -c71-71 data.dat | gzip -c - > data/vn09radio.gz
cut -c72-72 data.dat | gzip -c - > data/vn09phone.gz
cut -c73-73 data.dat | gzip -c - > data/vn09computr.gz
cut -c74-74 data.dat | gzip -c - > data/vn09washer.gz
cut -c75-75 data.dat | gzip -c - > data/vn09refrig.gz
cut -c76-76 data.dat | gzip -c - > data/vn09aircon.gz
cut -c77-77 data.dat | gzip -c - > data/vn09moto.gz
cut -c78-80 data.dat | gzip -c - > data/pernum.gz
cut -c81-88 data.dat | gzip -c - > data/wtper.gz
cut -c89-90 data.dat | gzip -c - > data/vn09pernum.gz
cut -c91-92 data.dat | gzip -c - > data/vn09personno.gz
cut -c93-93 data.dat | gzip -c - > data/vn09relate.gz
cut -c94-94 data.dat | gzip -c - > data/vn09sex.gz
cut -c95-96 data.dat | gzip -c - > data/vn09age.gz
cut -c97-97 data.dat | gzip -c - > data/vn09mig.gz
cut -c98-98 data.dat | gzip -c - > data/vn09urbmig.gz
cut -c99-99 data.dat | gzip -c - > data/vn09educ.gz
cut -c100-101 data.dat | gzip -c - > data/vn09edattan.gz
cut -c102-103 data.dat | gzip -c - > data/vn09grade.gz
cut -c104-107 data.dat | gzip -c - > data/vn09edattain.gz
cut -c108-108 data.dat | gzip -c - > data/vn09lit.gz
cut -c109-109 data.dat | gzip -c - > data/vn09marst.gz
cut -c110-111 data.dat | gzip -c - > data/vn09qualif.gz
cut -c112-112 data.dat | gzip -c - > data/vn09emp1.gz
cut -c113-113 data.dat | gzip -c - > data/vn09emp2.gz
cut -c114-114 data.dat | gzip -c - > data/vn09emp3.gz
cut -c115-117 data.dat | gzip -c - > data/vn09occ.gz
