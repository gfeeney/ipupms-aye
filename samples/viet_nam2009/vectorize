cut -c125-125 data.dat | gzip -c - > data/vn09reasue.gz
cut -c126-126 data.dat | gzip -c - > data/vn09birthx.gz
cut -c127-127 data.dat | gzip -c - > data/vn09disvis.gz
cut -c128-128 data.dat | gzip -c - > data/vn09dishear.gz
cut -c129-129 data.dat | gzip -c - > data/vn09diswalk.gz
cut -c130-130 data.dat | gzip -c - > data/vn09disrem.gz
cut -c131-132 data.dat | gzip -c - > data/vn09nchhh.gz
cut -c133-134 data.dat | gzip -c - > data/vn09nchelse.gz
cut -c135-136 data.dat | gzip -c - > data/vn09nchdead.gz
cut -c137-140 data.dat | gzip -c - > data/vn09lstbyr.gz
cut -c141-142 data.dat | gzip -c - > data/vn09lstbmo.gz
cut -c143-143 data.dat | gzip -c - > data/vn09lstbfem.gz
cut -c144-144 data.dat | gzip -c - > data/vn09lstbmale.gz
cut -c145-146 data.dat | gzip -c - > data/vn09month.gz
cut -c147-150 data.dat | gzip -c - > data/vn09birthyr.gz
cut -c151-151 data.dat | gzip -c - > data/vn09ethnic.gz
cut -c152-152 data.dat | gzip -c - > data/vn09relig.gz
cut -c153-155 data.dat | gzip -c - > data/vn09migdist.gz
cut -c156-157 data.dat | gzip -c - > data/vn09migprov.gz
