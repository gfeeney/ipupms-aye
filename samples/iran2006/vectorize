cut -c186-186 data.dat | gzip -c - > data/disabnum.gz
cut -c187-189 data.dat | gzip -c - > data/bpl2.gz
cut -c190-192 data.dat | gzip -c - > data/citiz2.gz
rm data.dat
echo "  End vectorizing: " `date`
