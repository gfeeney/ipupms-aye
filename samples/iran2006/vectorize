#!/bin/sh
# Vectorize data file
echo "Begin vectorizing: " `date`
gunzip -c source/ipumsi_00079.dat.gz > data.dat
sha1sum data.dat > source/ipumsi_00.sha1
mkdir data
cut -c1-3 data.dat | gzip -c - > data/cntry.gz
cut -c4-7 data.dat | gzip -c - > data/year.gz
cut -c8-11 data.dat | gzip -c - > data/sample.gz
cut -c12-21 data.dat | gzip -c - > data/serial.gz
cut -c22-22 data.dat | gzip -c - > data/rectype.gz
cut -c23-28 data.dat | gzip -c - > data/dwnum.gz
cut -c29-30 data.dat | gzip -c - > data/pern.gz
cut -c31-31 data.dat | gzip -c - > data/fbig.gz
cut -c32-33 data.dat | gzip -c - > data/fbig_nh.gz
cut -c34-35 data.dat | gzip -c - > data/prov.gz
cut -c36-36 data.dat | gzip -c - > data/urb.gz
cut -c37-37 data.dat | gzip -c - > data/hhtype.gz
cut -c38-38 data.dat | gzip -c - > data/motobike.gz
cut -c39-39 data.dat | gzip -c - > data/auto.gz
cut -c40-40 data.dat | gzip -c - > data/pc.gz
cut -c41-41 data.dat | gzip -c - > data/internet.gz
cut -c42-42 data.dat | gzip -c - > data/fuelcook.gz
cut -c43-43 data.dat | gzip -c - > data/fuelheat.gz
cut -c44-44 data.dat | gzip -c - > data/fuelwatr.gz
cut -c45-45 data.dat | gzip -c - > data/watdrink.gz
cut -c46-46 data.dat | gzip -c - > data/watcook.gz
cut -c47-47 data.dat | gzip -c - > data/watother.gz
cut -c48-48 data.dat | gzip -c - > data/dwtype.gz
cut -c49-50 data.dat | gzip -c - > data/rooms.gz
cut -c51-51 data.dat | gzip -c - > data/ownershp.gz
cut -c52-52 data.dat | gzip -c - > data/electric.gz
cut -c53-53 data.dat | gzip -c - > data/phone.gz
cut -c54-54 data.dat | gzip -c - > data/phoneuse.gz
cut -c55-55 data.dat | gzip -c - > data/pipewtr.gz
cut -c56-56 data.dat | gzip -c - > data/pipewtru.gz
cut -c57-57 data.dat | gzip -c - > data/pipegas.gz
cut -c58-58 data.dat | gzip -c - > data/pipegasu.gz
cut -c59-59 data.dat | gzip -c - > data/centheat.gz
cut -c60-60 data.dat | gzip -c - > data/centair.gz
