cut -c62-62 data.dat | gzip -c - > data/kitchenu.gz
cut -c63-63 data.dat | gzip -c - > data/bath.gz
cut -c64-64 data.dat | gzip -c - > data/bathuse.gz
cut -c65-65 data.dat | gzip -c - > data/toilet.gz
cut -c66-66 data.dat | gzip -c - > data/numhhs2.gz
cut -c67-68 data.dat | gzip -c - > data/numhhs.gz
cut -c69-70 data.dat | gzip -c - > data/roomshh.gz
cut -c71-72 data.dat | gzip -c - > data/area.gz
cut -c73-74 data.dat | gzip -c - > data/const.gz
cut -c75-78 data.dat | gzip -c - > data/yrbuilt.gz
cut -c79-79 data.dat | gzip -c - > data/wastetoi.gz
cut -c80-80 data.dat | gzip -c - > data/wastekit.gz
cut -c81-81 data.dat | gzip -c - > data/wasteoth.gz
cut -c82-83 data.dat | gzip -c - > data/persons.gz
cut -c84-85 data.dat | gzip -c - > data/workerhh.gz
