cut -c147-148 data.dat | gzip -c - > data/empstat.gz
cut -c149-152 data.dat | gzip -c - > data/occ4.gz
cut -c153-155 data.dat | gzip -c - > data/occ3.gz
cut -c156-159 data.dat | gzip -c - > data/ind4.gz
cut -c160-162 data.dat | gzip -c - > data/ind3.gz
cut -c163-163 data.dat | gzip -c - > data/classwk.gz
cut -c164-164 data.dat | gzip -c - > data/workloc.gz
cut -c165-165 data.dat | gzip -c - > data/marst.gz
cut -c166-167 data.dat | gzip -c - > data/marrnum.gz
cut -c168-168 data.dat | gzip -c - > data/everborn.gz
