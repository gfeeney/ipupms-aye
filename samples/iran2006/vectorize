cut -c90-91 data.dat | gzip -c - > data/illithh.gz
cut -c92-93 data.dat | gzip -c - > data/mstudhh.gz
cut -c94-95 data.dat | gzip -c - > data/fstudhh.gz
cut -c96-98 data.dat | gzip -c - > data/pernum.gz
cut -c99-106 data.dat | gzip -c - > data/wtper.gz
cut -c107-108 data.dat | gzip -c - > data/pernum.gz
cut -c109-110 data.dat | gzip -c - > data/perno.gz
cut -c111-112 data.dat | gzip -c - > data/relate.gz
cut -c113-113 data.dat | gzip -c - > data/mother.gz
