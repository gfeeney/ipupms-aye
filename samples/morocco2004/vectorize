#!/bin/sh
# Vectorize data file
echo "Begin vectorizing: " `date`
gunzip -c source/ipumsi_00094.dat.gz > data.dat
sha1sum data.dat > source/ipumsi_00094.dat.gz.sha1
mkdir data
cut -c1-3 data.dat | gzip -c - > data/cntry.gz
cut -c4-7 data.dat | gzip -c - > data/year.gz
cut -c8-11 data.dat | gzip -c - > data/sample.gz
cut -c12-21 data.dat | gzip -c - > data/serial.gz
cut -c22-27 data.dat | gzip -c - > data/dwnum.gz
cut -c28-29 data.dat | gzip -c - > data/pern.gz
cut -c30-31 data.dat | gzip -c - > data/region.gz
cut -c32-34 data.dat | gzip -c - > data/prov.gz
cut -c35-35 data.dat | gzip -c - > data/recordty.gz
cut -c36-36 data.dat | gzip -c - > data/tents.gz
cut -c37-37 data.dat | gzip -c - > data/light.gz
cut -c38-38 data.dat | gzip -c - > data/cattle.gz
cut -c39-39 data.dat | gzip -c - > data/sheep.gz
cut -c40-40 data.dat | gzip -c - > data/occup.gz
cut -c41-42 data.dat | gzip -c - > data/constrty.gz
cut -c43-43 data.dat | gzip -c - > data/ownershp.gz
cut -c44-44 data.dat | gzip -c - > data/dwellage.gz
cut -c45-46 data.dat | gzip -c - > data/rooms.gz
cut -c47-47 data.dat | gzip -c - > data/kitchuse.gz
cut -c48-48 data.dat | gzip -c - > data/toilet.gz
cut -c49-49 data.dat | gzip -c - > data/bath.gz
cut -c50-50 data.dat | gzip -c - > data/bathtrad.gz
cut -c51-51 data.dat | gzip -c - > data/watuse.gz
cut -c52-52 data.dat | gzip -c - > data/elect.gz
cut -c53-53 data.dat | gzip -c - > data/ltpetrol.gz
cut -c54-54 data.dat | gzip -c - > data/ltgas.gz
cut -c55-55 data.dat | gzip -c - > data/ltcandle.gz
cut -c56-56 data.dat | gzip -c - > data/ltsolar.gz
cut -c57-57 data.dat | gzip -c - > data/ltgener.gz
cut -c58-58 data.dat | gzip -c - > data/ltother.gz
cut -c59-59 data.dat | gzip -c - > data/watwell.gz
cut -c60-60 data.dat | gzip -c - > data/watsprng.gz
cut -c61-61 data.dat | gzip -c - > data/watfount.gz
cut -c62-62 data.dat | gzip -c - > data/watother.gz
cut -c63-63 data.dat | gzip -c - > data/sewage.gz
cut -c64-64 data.dat | gzip -c - > data/septic.gz
cut -c65-65 data.dat | gzip -c - > data/pit.gz
cut -c66-66 data.dat | gzip -c - > data/othtoil.gz
cut -c67-67 data.dat | gzip -c - > data/tv.gz
cut -c68-68 data.dat | gzip -c - > data/tvdish.gz
cut -c69-69 data.dat | gzip -c - > data/phone.gz
cut -c70-72 data.dat | gzip -c - > data/distroad.gz
cut -c73-75 data.dat | gzip -c - > data/pernum.gz
cut -c76-83 data.dat | gzip -c - > data/wtper.gz
cut -c84-85 data.dat | gzip -c - > data/pernum.gz
cut -c86-87 data.dat | gzip -c - > data/lineno.gz
cut -c88-88 data.dat | gzip -c - > data/resident.gz
cut -c89-90 data.dat | gzip -c - > data/nation.gz
cut -c91-92 data.dat | gzip -c - > data/relate.gz
cut -c93-93 data.dat | gzip -c - > data/sex.gz
cut -c94-95 data.dat | gzip -c - > data/birthmo.gz
cut -c96-99 data.dat | gzip -c - > data/birthyrg.gz
cut -c100-103 data.dat | gzip -c - > data/birthyrh.gz
cut -c104-105 data.dat | gzip -c - > data/age.gz
cut -c106-106 data.dat | gzip -c - > data/marst.gz
cut -c107-108 data.dat | gzip -c - > data/chhome.gz
cut -c109-110 data.dat | gzip -c - > data/chaway.gz
cut -c111-112 data.dat | gzip -c - > data/chdead.gz
cut -c113-113 data.dat | gzip -c - > data/chbornyr.gz
cut -c114-114 data.dat | gzip -c - > data/chdeadyr.gz
cut -c115-115 data.dat | gzip -c - > data/lit.gz
cut -c116-116 data.dat | gzip -c - > data/edlevel.gz
cut -c117-118 data.dat | gzip -c - > data/edlevel2.gz
cut -c119-119 data.dat | gzip -c - > data/schoolty.gz
cut -c120-120 data.dat | gzip -c - > data/lang1.gz
cut -c121-121 data.dat | gzip -c - > data/lang2.gz
cut -c122-122 data.dat | gzip -c - > data/disab.gz
cut -c123-123 data.dat | gzip -c - > data/empstat.gz
cut -c124-125 data.dat | gzip -c - > data/classwk.gz
cut -c126-128 data.dat | gzip -c - > data/occ3.gz
cut -c129-130 data.dat | gzip -c - > data/occ1.gz
cut -c131-133 data.dat | gzip -c - > data/occ2.gz
cut -c134-136 data.dat | gzip -c - > data/ind3.gz
cut -c137-139 data.dat | gzip -c - > data/ind2.gz
cut -c140-141 data.dat | gzip -c - > data/degree2.gz
cut -c142-144 data.dat | gzip -c - > data/degree3.gz
cut -c145-147 data.dat | gzip -c - > data/prevprov.gz
cut -c148-149 data.dat | gzip -c - > data/prevregn.gz
cut -c150-151 data.dat | gzip -c - > data/durres.gz
cut -c152-154 data.dat | gzip -c - > data/prov5yr.gz
cut -c155-156 data.dat | gzip -c - > data/regn5yr.gz
rm data.dat
echo "  End vectorizing: " `date`
