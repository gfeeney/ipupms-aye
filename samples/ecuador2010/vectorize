#!/bin/sh
# Vectorize data file
echo "Begin vectorizing: " `date`
gunzip -c source/ipumsi_00067.dat.gz > data.dat
sha1sum data.dat > source/ipumsi_00067.dat.gz.sha1
mkdir data
