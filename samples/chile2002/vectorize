cut -c33-34 data.dat | gzip -c - > data/pernd.gz
cut -c35-36 data.dat | gzip -c - > data/pern.gz
cut -c37-37 data.dat | gzip -c - > data/fbig.gz
cut -c38-39 data.dat | gzip -c - > data/region.gz
cut -c40-40 data.dat | gzip -c - > data/urban2.gz
cut -c41-42 data.dat | gzip -c - > data/dwtype.gz
cut -c43-43 data.dat | gzip -c - > data/occupan.gz
cut -c44-44 data.dat | gzip -c - > data/owner.gz
cut -c45-45 data.dat | gzip -c - > data/wall.gz
