cut -c51-51 data.dat | gzip -c - > data/toilet.gz
cut -c52-52 data.dat | gzip -c - > data/nshowers.gz
cut -c53-54 data.dat | gzip -c - > data/rooms.gz
cut -c55-56 data.dat | gzip -c - > data/hhno.gz
cut -c57-58 data.dat | gzip -c - > data/rmbed.gz
cut -c59-60 data.dat | gzip -c - > data/rmlivdin.gz
cut -c61-62 data.dat | gzip -c - > data/rmlive.gz
cut -c63-64 data.dat | gzip -c - > data/rmdine.gz
cut -c65-66 data.dat | gzip -c - > data/rmkitch.gz
cut -c67-68 data.dat | gzip -c - > data/rmwork.gz
cut -c69-70 data.dat | gzip -c - > data/rmstudy.gz
