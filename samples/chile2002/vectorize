cut -c148-150 data.dat | gzip -c - > data/edattain.gz
cut -c151-151 data.dat | gzip -c - > data/marst.gz
cut -c152-152 data.dat | gzip -c - > data/relig.gz
cut -c153-154 data.dat | gzip -c - > data/empstat.gz
