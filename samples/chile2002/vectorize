cut -c95-95 data.dat | gzip -c - > data/van.gz
cut -c96-96 data.dat | gzip -c - > data/auto.gz
cut -c97-97 data.dat | gzip -c - > data/truck.gz
cut -c98-98 data.dat | gzip -c - > data/boat.gz
cut -c99-99 data.dat | gzip -c - > data/hhtype.gz
