cut -c79-79 data.dat | gzip -c - > data/vcr.gz
cut -c80-80 data.dat | gzip -c - > data/tvcable.gz
cut -c81-81 data.dat | gzip -c - > data/stereo.gz
cut -c82-82 data.dat | gzip -c - > data/washer.gz
cut -c83-83 data.dat | gzip -c - > data/dryer.gz
cut -c84-84 data.dat | gzip -c - > data/refrig.gz
cut -c85-85 data.dat | gzip -c - > data/freez.gz
cut -c86-86 data.dat | gzip -c - > data/micro.gz
