cut -c53-53 data.dat | gzip -c - > data/toilet.gz
cut -c54-54 data.dat | gzip -c - > data/facil.gz
cut -c55-55 data.dat | gzip -c - > data/heat.gz
cut -c56-56 data.dat | gzip -c - > data/fuelht.gz
cut -c57-58 data.dat | gzip -c - > data/story.gz
cut -c59-59 data.dat | gzip -c - > data/owner1.gz
