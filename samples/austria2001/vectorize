cut -c30-30 data.dat | gzip -c - > data/fbig.gz
cut -c31-31 data.dat | gzip -c - > data/donated.gz
cut -c32-32 data.dat | gzip -c - > data/d_num.gz
cut -c33-34 data.dat | gzip -c - > data/region.gz
cut -c35-35 data.dat | gzip -c - > data/hhty1.gz
cut -c36-36 data.dat | gzip -c - > data/hhty2.gz
cut -c37-37 data.dat | gzip -c - > data/hhty3.gz
cut -c38-38 data.dat | gzip -c - > data/hhty4.gz
cut -c39-40 data.dat | gzip -c - > data/hhty5.gz
cut -c41-42 data.dat | gzip -c - > data/hhty6.gz
