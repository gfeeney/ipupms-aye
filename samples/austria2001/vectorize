cut -c82-82 data.dat | gzip -c - > data/sex.gz
cut -c83-83 data.dat | gzip -c - > data/marst.gz
cut -c84-84 data.dat | gzip -c - > data/cohab.gz
cut -c85-86 data.dat | gzip -c - > data/citiz.gz
cut -c87-88 data.dat | gzip -c - > data/bplc.gz
cut -c89-90 data.dat | gzip -c - > data/religd.gz
