cut -c148-149 data.dat | gzip -c - > data/relate.gz
cut -c150-151 data.dat | gzip -c - > data/hhstat4.gz
cut -c152-152 data.dat | gzip -c - > data/famst1.gz
cut -c153-153 data.dat | gzip -c - > data/famst2.gz
cut -c154-154 data.dat | gzip -c - > data/famst3.gz
